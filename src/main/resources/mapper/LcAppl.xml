<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.taikang.wechat.dao.applUser.ApplUserDao">

    <select id="isExistEmaile" resultType="java.lang.String">
      select userId from appl_user where userEmail=#{userEmail}
    </select>

    <select id="isExistPhone" resultType="java.lang.String">
      select userId from appl_user where userPhone=#{userPhone}
    </select>

</mapper>