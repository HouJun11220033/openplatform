<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.taikang.wechat.dao.UserInfoDao">

    <select id="selectNameById" resultType="java.lang.String">
      select NAME from mytest where ID=#{id}
    </select>
    <select id="select" resultType="com.taikang.wechat.model.UserInfo">
      select * from mytest
    </select>
</mapper>